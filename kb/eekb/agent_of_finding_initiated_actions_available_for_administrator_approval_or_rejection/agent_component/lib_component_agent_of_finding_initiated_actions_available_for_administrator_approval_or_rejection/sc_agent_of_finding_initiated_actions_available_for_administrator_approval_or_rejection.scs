sc_agent_of_finding_initiated_actions_available_for_administrator_approval_or_rejection
=> nrel_main_idtf:
	[sc-агент поиска инициированных действий, доступных администратору для отклонения или утверждения] (* <- lang_ru;; *);
	[sc-agent of finding initiated actions available for administrator approval or rejection] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_finding_initiated_actions_available_for_administrator_approval_or_rejection_condition => ..sc_agent_of_finding_initiated_actions_available_for_administrator_approval_or_rejection_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_finding_initiated_actions_available_for_administrator_approval_or_rejection
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_finding_initiated_actions_available_for_administrator_approval_or_rejection
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_finding_initiated_actions_available_for_administrator_approval_or_rejection	
		};;

	-> sc_agent_of_finding_initiated_actions_available_for_administrator_approval_or_rejection_scp    
		(* <- active_sc_agent;; *);;  
	*);;

..sc_agent_of_finding_initiated_actions_available_for_administrator_approval_or_rejection_condition
= [*
	action_of_finding_initiated_actions_available_for_administrator_approval_or_rejection _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_finding_initiated_actions_available_for_administrator_approval_or_rejection_result
= [*
	action_of_finding_initiated_actions_available_for_administrator_approval_or_rejection _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
