sc_agent_of_checking_subject_domain_existence_for_concepts
=> nrel_main_idtf:
	[sc-агент поиска понятий, которые не соотнесены с предметной областью] (* <- lang_ru;; *);
	[sc-agent of checking subject domain existence for concepts] (* <- lang_en;; *);
	
<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);
	
=> nrel_initiation_condition_and_result: 
	(..sc_agent_of_checking_subject_domain_existence_for_concepts_condition => ..sc_agent_of_checking_subject_domain_existence_for_concepts_result);
	
<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	action_of_checking_subject_domain_existence_for_concepts
	};
	
=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_of_checking_subject_domain_existence_for_concepts
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_of_checking_subject_domain_existence_for_concepts;
		proc_of_generation_of_verification_answer
		};;
		
	-> sc_agent_of_checking_subject_domain_existence_for_concepts_scp 
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_of_checking_subject_domain_existence_for_concepts_condition
= [*
	action_of_checking_subject_domain_existence_for_concepts _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_of_checking_subject_domain_existence_for_concepts_result
= [*
	action_of_checking_subject_domain_existence_for_concepts _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
